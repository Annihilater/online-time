{
  "dashboard": {
    "id": null,
    "title": "在线时间工具 - 应用概览",
    "tags": ["online-time", "application"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "应用状态",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(up{job=\"online-time-app\"})",
            "legendFormat": "在线实例数"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "请求速率",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"online-time-app\"}[5m]))",
            "legendFormat": "req/s"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "错误率",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"online-time-app\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"online-time-app\"}[5m])) * 100",
            "legendFormat": "错误率 %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "响应时间 P95",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"online-time-app\"}[5m])) by (le))",
            "legendFormat": "P95 响应时间"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "请求趋势",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (instance) (rate(http_requests_total{job=\"online-time-app\"}[5m]))",
            "legendFormat": "{{instance}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "yAxes": [
          {
            "label": "请求/秒",
            "show": true
          }
        ]
      },
      {
        "id": 6,
        "title": "响应时间分布",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"online-time-app\"}[5m])) by (le))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"online-time-app\"}[5m])) by (le))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"online-time-app\"}[5m])) by (le))",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "label": "时间 (秒)",
            "show": true
          }
        ]
      },
      {
        "id": 7,
        "title": "HTTP状态码分布",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (status) (rate(http_requests_total{job=\"online-time-app\"}[5m]))",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "stack": true
      },
      {
        "id": 8,
        "title": "活跃用户数",
        "type": "graph",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "活跃用户"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}